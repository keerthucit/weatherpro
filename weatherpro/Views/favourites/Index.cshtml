@model IEnumerable<weatherpro.Models.DB.favourite>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<br />
<br />
<br /><br />
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.place.city)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.register.username)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.place.city)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.register.username)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.fid }) |
            @Html.ActionLink("Details", "Details", new { id=item.fid }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.fid })
        </td>
    </tr>
}

</table>
